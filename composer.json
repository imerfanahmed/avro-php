{
    "name": "eru/avro-phonetic",
    "description": "Avro Phonetic - Bengali/Bangla transliteration library for PHP and Laravel",
    "keywords": ["avro", "phonetic", "bangla", "bengali", "transliteration", "laravel"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Erfan Ahmed Siam",
            "email": "erfan.siam98@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Eru\\AvroPhonetic\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eru\\AvroPhonetic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eru\\AvroPhonetic\\AvroPhoneticServiceProvider"
            ],
            "aliases": {
                "Avro": "Eru\\AvroPhonetic\\Facades\\Avro"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
